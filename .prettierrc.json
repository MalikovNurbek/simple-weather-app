{
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 100,
    "arrowParens": "avoid",
    "vue/attributes-order": ["error", {
        "order": [
            "DEFINITION",
            "LIST_RENDERING",
            "CONDITIONALS",
            "RENDER_MODIFIERS",
            "GLOBAL",
            ["UNIQUE", "SLOT"],
            "TWO_WAY_BINDING",
            "OTHER_DIRECTIVES",
            "OTHER_ATTR",
            "EVENTS",
            "CONTENT"
        ],
        "alphabetical": false
    }]
}
